<?xml version="1.0" encoding="UTF-8"?>
<Messages mongoSupport="true">


	<ConstantGroups prefix="Vehicle">
		<Group name="Type">
			<Constant id="Car"/>
			<Constant id="Truck"/>
			<Constant id="Motorbike"/>
		</Group>
	</ConstantGroups>



	<Message name="Vehicle">
		<Attribute name="type" constantGroup="Type"/>
		<Attribute name="licensPlate" type="String"/>
		<Attribute name="insurensNumber" type="String"/>
		<Attribute name="wheels" type="int"/>
	</Message>



	<Message name="Car" extends="Vehicle" implements="FrotzIf" rootMessage="true" db="true" dbCollection="Car">
		<Imports>
			<Import path="com.hoddmimes.dummy.*"/>
		</Imports>
		<Interfaces>
			<Interface name="FooIf"/>
			<Interface name="BarIf"/>
		</Interfaces>
		<Attribute name="color" type="String"/>
		<Attribute name="airbags" type="boolean"/>
		<Attribute name="seats" type="int"/>
		<Attribute name="manufactor" type="String" dbKey="not-unique"/>
		<Attribute name="productionYear" type="int" dbKey="not-unique"/>
	</Message>
</Messages>
